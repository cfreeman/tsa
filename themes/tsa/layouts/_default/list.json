{
  "cards": [
  {{ $section := where .Site.RegularPages "Section" "in" "posts" }}
  {{ range $i, $e := $section }}
  {{ if $i }}, {{end}} {
    "Title": "{{ .Title }}",
    "Description": "{{ .Description }}",
    "Date": "{{ .Date }}",
    "Videos": {{ jsonify .Params.Videos }},
    "Images": {{ jsonify .Params.Images }},
    "URL": "{{ .Permalink }}"
    }
  {{end}}
  ]
}



